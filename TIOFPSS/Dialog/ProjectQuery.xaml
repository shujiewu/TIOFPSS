                <!--<xcdg:DataGridCollectionViewSource.GroupDescriptions>
                    <PropertyGroupDescription PropertyName="ProjectPath" />
                    <PropertyGroupDescription PropertyName="ProjectName" />
                </xcdg:DataGridCollectionViewSource.GroupDescriptions>-->
