<Window x:Class="TIOFPSS.Dialog.DongTaiFenXi"
         xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="动态分析" Height="340.379" Width="450"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <GroupBox Header="参数信息" Margin="10">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition />
                </Grid.RowDefinitions>
                <TextBox x:Name="zengsushijian"  Grid.Row="0" HorizontalAlignment="Left" Height="23" Margin="142,20,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="233"/>
                <TextBlock HorizontalAlignment="Left" Grid.Row="0" Margin="35,22,0,0" TextWrapping="Wrap" Text="增速时间：" VerticalAlignment="Top" />
                <TextBox x:Name="wendnigshijian"  Grid.Row="1" HorizontalAlignment="Left" Height="23" Margin="142,20,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="233"/>
                <TextBlock HorizontalAlignment="Left" Grid.Row="1" Margin="35,22,0,0" TextWrapping="Wrap" Text="稳定时间：" VerticalAlignment="Top" />

                <TextBox x:Name="fanzhengshijian"  Grid.Row="2" HorizontalAlignment="Left" Height="23" Margin="142,20,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="233"/>
                <TextBlock HorizontalAlignment="Left" Grid.Row="2" Margin="35,22,0,0" TextWrapping="Wrap" Text="仿真结束时间：" VerticalAlignment="Top" />
                <TextBox x:Name="zhiLiang"  Grid.Row="3" HorizontalAlignment="Left" Height="23" Margin="142,20,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="233"/>
                <TextBlock HorizontalAlignment="Left" Grid.Row="3" Margin="5,20,0,0" TextWrapping="Wrap" Text="与内毂连接轴的质量（吨）：" VerticalAlignment="Top" />
                <TextBox x:Name="resName"  Grid.Row="4" HorizontalAlignment="Left" Height="23" Margin="142,20,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="233"/>
                <TextBlock HorizontalAlignment="Left" Grid.Row="4" Margin="35,22,0,0" TextWrapping="Wrap" Text="结果文件命名：" VerticalAlignment="Top" />
                <Button Content="开始分析" Grid.Row="5" HorizontalAlignment="Left" Margin="224,22,0,0" VerticalAlignment="Top" Width="70" Height="23" Click="OnOKClick"/>
                <Button Content="取消"  Grid.Row="5" HorizontalAlignment="Left" Margin="304,22,0,0" VerticalAlignment="Top" Width="70" Height="23" Click="OnCancelClick"/>

            </Grid>
        </GroupBox>
    </Grid>
</Window>
