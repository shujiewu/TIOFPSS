             <Setter Property="BorderBrush" Value="Black" />
             <Setter Property="BorderThickness" Value="1" />
             <Setter Property="Padding" Value="3,6,0,6" />
         </Style>
         <Style x:Key="ExceptionHeaderStyle" TargetType="{x:Type Label}">
